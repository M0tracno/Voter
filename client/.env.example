# FastVerify Client Environment Configuration

# API Configuration
REACT_APP_API_BASE_URL=http://localhost:5000/api
REACT_APP_WEBSOCKET_URL=ws://localhost:5000

# Application Configuration
REACT_APP_APP_NAME=FastVerify
REACT_APP_APP_VERSION=2.0.0
REACT_APP_ENVIRONMENT=development

# Features
REACT_APP_ENABLE_PWA=true
REACT_APP_ENABLE_OFFLINE=true
REACT_APP_ENABLE_ANALYTICS=false
REACT_APP_ENABLE_ERROR_REPORTING=false

# Security
REACT_APP_SESSION_TIMEOUT=3600000
REACT_APP_IDLE_TIMEOUT=1800000

# UI Configuration
REACT_APP_DEFAULT_THEME=light
REACT_APP_DEFAULT_LANGUAGE=en

# QR Code Scanner
REACT_APP_QR_SCAN_FPS=10
REACT_APP_QR_SCAN_ASPECT_RATIO=1.0

# Cache Configuration
REACT_APP_CACHE_DURATION=300000
REACT_APP_MAX_CACHE_SIZE=50

# Development
REACT_APP_ENABLE_DEBUG=true
REACT_APP_LOG_LEVEL=debug

# Analytics (if enabled)
# REACT_APP_GOOGLE_ANALYTICS_ID=
# REACT_APP_MIXPANEL_TOKEN=

# Error Reporting (if enabled)
# REACT_APP_SENTRY_DSN=
